   2     3    1       0.27    
   2     4    1       0.27   
   3     4    1       0.27  

[angles]
#meta {"group": "Side chain angles"}
;  i     j    k     funct   angle  force.c.
    1     2    3       2   150.000   50.0  
    1     2    4       2   150.000   50.0 

[dihedrals]
;  i     j    k    l   funct   angle  force.c.
   1     3    4    2       2    0.0    50.0     ; to prevent backflipping of ring

